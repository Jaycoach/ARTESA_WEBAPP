option_settings:
  aws:elasticbeanstalk:container:nodejs:
    ProxyServer: nginx
  aws:elasticbeanstalk:environment:staticfiles:
    /static: public
    /uploads: uploads
  aws:elasticbeanstalk:environment:process:default:
    HealthCheckPath: /api/health
    Port: 8080
    Protocol: HTTP
    MatcherHTTPCode: 200