{
  "name": "loginartesa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run sass:build && vite",
    "build": "npm run sass:build && vite build",
    "sass:build": "node --no-warnings scripts/generate-css.js",
    "sass:watch": "sass --watch src:src",
    "lint": "eslint .",
    "preview": "vite preview",
    "purge": "purgecss --config purgecss.config.js --output dist/css/",
    "build:prod": "npm run build && npm run purge"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.12",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.5",
    "recharts": "^2.15.1",
    "tailwind-scrollbar": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "glob": "^10.3.10",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "sass": "^1.85.1",
    "tailwindcss": "^3.4.17",
    "vite": "^6.1.1"
  }
